{
  "name": "walkamongus-pdagent",
  "version": "0.1.1",
  "author": "Chadwick Banning",
  "summary": "Puppet module for installing and managing the PagerDuty agent",
  "license": "Apache-2.0",
  "source": "git@github.com:walkamongus/puppet-pdagent.git",
  "project_page": "https://github.com/walkamongus/puppet-pdagent",
  "issues_url": "https://github.com/walkamongus/puppet-pdagent/issues",
  "tags": [ "pagerduty", "pdagent" ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.5.1"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">=4.0.0 <5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">=1.0.0 <2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    }
  ]
}
